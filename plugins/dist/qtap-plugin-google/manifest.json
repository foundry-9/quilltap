{
  "$schema": "../qtap-plugin-template/schemas/plugin-manifest.schema.json",
  "name": "qtap-plugin-google",
  "title": "Google Gemini Provider",
  "description": "Provides Google Gemini models and Imagen image generation for Quilltap",
  "version": "1.0.0",
  "author": {
    "name": "Foundry-9",
    "email": "charles@sebold.tech",
    "url": "https://foundry-9.com"
  },
  "license": "MIT",
  "compatibility": {
    "quilltapVersion": ">=1.7.0",
    "nodeVersion": ">=18.0.0"
  },
  "capabilities": ["LLM_PROVIDER", "IMAGE_PROVIDER"],
  "category": "PROVIDER",
  "main": "index.js",
  "typescript": true,
  "frontend": "REACT",
  "styling": "TAILWIND",
  "enabledByDefault": true,
  "status": "STABLE",
  "keywords": ["google", "gemini", "imagen", "llm", "ai", "chat", "image-generation"],
  "providerConfig": {
    "providerName": "GOOGLE",
    "displayName": "Google Gemini",
    "description": "Google Gemini models including text and image generation via Generative AI API",
    "abbreviation": "GGL",
    "colors": {
      "bg": "bg-blue-100",
      "text": "text-blue-800",
      "icon": "text-blue-600"
    },
    "requiresApiKey": true,
    "requiresBaseUrl": false,
    "apiKeyLabel": "Google Generative AI API Key",
    "capabilities": {
      "chat": true,
      "imageGeneration": true,
      "embeddings": false,
      "webSearch": true
    },
    "attachmentSupport": {
      "supported": true,
      "mimeTypes": ["image/jpeg", "image/png", "image/gif", "image/webp"],
      "description": "Images only (JPEG, PNG, GIF, WebP)"
    }
  },
  "permissions": {
    "network": ["generativelanguage.googleapis.com"],
    "userData": false,
    "database": false
  }
}
